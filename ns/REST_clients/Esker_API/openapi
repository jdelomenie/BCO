  /v1/file:
    post:
      tags:
      - File
      summary: Uploads a file for subsequent usage in other functions via its ID.
      description: "Uploaded file will be kept during 15 minutes.\r\nMaximal size\
        \ is 50 Mo.\r\nRefer to <a href=\"https://doc.esker.com/eskerondemand/cv_ly/en/manager/startpage.htm#REST/POST_file.html\"\
        >REST Web services API: POST file</a> for more information."
      requestBody:
        content:
          multipart/form-data:
            schema:
              type: object
              properties:
                uploadedFile:
                  type: string
                  format: binary
            encoding:
              uploadedFile:
                style: form
      responses:
        "200":
          description: Success
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResourceFile'
        "400":
          description: Bad Request
          content:
            application/json:
              schema:
                type: string
        "401":
          description: Unauthorized - You need to make a new authentication (call
            token route)
          content:
            application/json:
              schema:
                type: string
        "413":
          description: The page was not displayed because the request entity is too
            large.
          content:
            application/json:
              schema:
                type: string
        "429":
          description: Too Many Requests
          content:
            application/json:
              schema:
                type: string
        "500":
          description: Internal Server Error
          content:
            application/json:
              schema:
                type: string
