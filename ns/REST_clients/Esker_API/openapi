  /v1/process/{processName}:
    get:
      tags:
      - Process
      summary: Initiates a query request on all instances of the given process. You
        can specify a filter and the fields you want to retrieve.
      description: "Refer to <a href=\"https://doc.esker.com/eskerondemand/cv_ly/en/manager/startpage.htm#REST/GET_process_processName.html\"\
        >REST Web services API: GET process/processName</a> for more information."
      parameters:
      - name: processName
        in: path
        description: " String value. Name of the process you want to query the instances\
          \ from.\r\n \r\n\tExample: Customer Order Processing"
        required: true
        style: simple
        explode: false
        schema:
          type: string
      - name: fields
        in: query
        description: " String value. Name of the fields you want to query from the\
          \ returned instances.\r\n \r\n\tExample: State,CustomerNumber__,SubmitDateTime"
        required: false
        style: form
        explode: true
        schema:
          type: string
          default: ""
      - name: filter
        in: query
        description: "String value. LDAP filter value to apply to the query.\r\nThe\
          \ filter value can not exceed 1000 characters.\r\n             \r\n\tExample:\
          \ State=70"
        required: false
        style: form
        explode: true
        schema:
          type: string
          default: ""
      - name: orderBy
        in: query
        description: "String value. Name of the field followed by the sorting order\
          \ keyword: ASC for ascending (default value) and DESC for descending. You\
          \ can sort by several fields, each followed by the sorting keyword, and\
          \ separated by commas.\r\n             \r\n\tExamples: SubmitDateTime DESC\r\
          \n\tCustomerNumber__, Total__ DESC"
        required: false
        style: form
        explode: true
        schema:
          type: string
          default: ""
      - name: limit
        in: query
        description: Integer value. Maximum number of results to retrieve (500 max
          or 20 max if tables or attachment options are set to true).
        required: false
        style: form
        explode: true
        schema:
          maximum: 500
          minimum: 1
          type: integer
          format: int32
          default: 20
      - name: onlyUnretrieved
        in: query
        description: ""
        required: false
        style: form
        explode: true
        schema:
          type: boolean
          default: false
      - name: tables
        in: query
        description: If this option is true the limit can't be superior to 20.
        required: false
        style: form
        explode: true
        schema:
          type: boolean
          default: false
      - name: attachments
        in: query
        description: If this option is true the limit can't be superior to 20.
        required: false
        style: form
        explode: true
        schema:
          type: boolean
          default: false
      - name: archived
        in: query
        description: " Boolean value. Indicates whether the archived process instances\
          \ are listed in the result or not.\r\n \r\n\t   true: the archived process\
          \ instances are listed.\r\n\t   false: the archived process instances are\
          \ not listed (default)."
        required: false
        style: form
        explode: true
        schema:
          type: boolean
          default: false
      responses:
        "200":
          description: Success
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ProcessInstances'
        "400":
          description: Bad Request
          content:
            application/json:
              schema:
                type: string
        "404":
          description: Not Found
          content:
            application/json:
              schema:
                type: string
        "401":
          description: Unauthorized - You need to make a new authentication (call
            token route)
          content:
            application/json:
              schema:
                type: string
        "429":
          description: Too Many Requests
          content:
            application/json:
              schema:
                type: string
        "500":
          description: Internal Server Error
          content:
            application/json:
              schema:
                type: string
    post:
      tags:
      - Process
      summary: Creates a new instance of a process from the given name of the process.
      description: "Refer to <a href=\"https://doc.esker.com/eskerondemand/cv_ly/en/manager/startpage.htm#REST/POST_process_processName.html\"\
        >REST Web services API: POST process/processName</a> for more information."
      parameters:
      - name: processName
        in: path
        description: " String value. Name of the process to instantiate.\r\n\r\n\t\
          Example: Customer Order Processing"
        required: true
        style: simple
        explode: false
        schema:
          type: string
      requestBody:
        description: ""
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/NewProcessInstance'
          text/json:
            schema:
              $ref: '#/components/schemas/NewProcessInstance'
          application/*+json:
            schema:
              $ref: '#/components/schemas/NewProcessInstance'
      responses:
        "201":
          description: Created
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ProcessInstanceCreation'
        "400":
          description: Bad Request
          content:
            application/json:
              schema:
                type: string
        "401":
          description: Unauthorized - You need to make a new authentication (call
            token route)
          content:
            application/json:
              schema:
                type: string
        "403":
          description: Forbidden - Insufficient privileges
          content:
            application/json:
              schema:
                type: string
        "429":
          description: Too Many Requests
          content:
            application/json:
              schema:
                type: string
        "500":
          description: Internal Server Error
          content:
            application/json:
              schema:
                type: string
  /v1/process/{processName}/{instanceId}:
    get:
      tags:
      - Process
      summary: Initiates a query request on one given process instance.
      description: "Refer to <a href=\"https://doc.esker.com/eskerondemand/cv_ly/en/manager/startpage.htm#REST/GET_process_processName_id.html\"\
        >REST Web services API: GET process/processName/instanceId</a> for more information."
      parameters:
      - name: processName
        in: path
        description: " String value. Name of the process you want to query the instance\
          \ from.\r\n \r\n\tExample: Customer Order Processing"
        required: true
        style: simple
        explode: false
        schema:
          type: string
      - name: instanceId
        in: path
        description: "String value. MsnEx identifier of the process instance you want\
          \ to query.\r\n             \r\n\tExample: 949978050000000001"
        required: true
        style: simple
        explode: false
        schema:
          type: string
      - name: tables
        in: query
        description: " Boolean value. Indicates whether the table fields contained\
          \ in the process instance are listed in the result or not.\r\n \r\n\t  \
          \ true: the table fields are listed.\r\n\t   false: the table fields are\
          \ not listed (default)."
        required: false
        style: form
        explode: true
        schema:
          type: boolean
      - name: summary
        in: query
        description: " Boolean value. Indicates whether the technical fields and the\
          \ fields with a null value are listed in the result or not.\r\n \r\n\t \
          \  true: the technical fields and the fields with a null value are not listed.\r\
          \n\t   false: the technical fields and the fields with a null value are\
          \ listed (default)."
        required: false
        style: form
        explode: true
        schema:
          type: boolean
      - name: archived
        in: query
        description: " Boolean value. Indicates whether the archived process instance\
          \ is listed in the result or not.\r\n \r\n\t   true: the archived process\
          \ instance is listed.\r\n\t   false: the archived process instance is not\
          \ listed (default)."
        required: false
        style: form
        explode: true
        schema:
          type: boolean
          default: false
      responses:
        "200":
          description: Success
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ProcessInstance'
        "404":
          description: Not Found
          content:
            application/json:
              schema:
                type: string
        "401":
          description: Unauthorized - You need to make a new authentication (call
            token route)
          content:
            application/json:
              schema:
                type: string
        "400":
          description: Bad Request
          content:
            application/json:
              schema:
                type: string
        "429":
          description: Too Many Requests
          content:
            application/json:
              schema:
                type: string
        "500":
          description: Internal Server Error
          content:
            application/json:
              schema:
                type: string
