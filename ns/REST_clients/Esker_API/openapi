openapi: 3.0.1
info:
  title: Esker on demand REST API
  version: v1
servers:
- url: https://we4.ondemand.esker.com/api
security:
- OAuth2: []
  BasicAuthentication: []
paths:
  /v1/token:
    post:
      tags:
      - Authentication
      summary: Authenticates the user credentials and gets an authentication token.
      description: "Refer to <a href=\"https://doc.esker.com/eskerondemand/cv_ly/en/manager/startpage.htm#REST/POST_token.html\"\
        >REST Web services API: POST token</a> for more information."
      requestBody:
        content:
          application/x-www-form-urlencoded:
            schema:
              required:
              - grant_type
              type: object
              properties:
                grant_type:
                  type: string
                  example: client_credentials
                client_id:
                  type: string
                  example: YOUR-EOD-IDENTIFIER
                client_secret:
                  type: string
                  example: YOUR-PASSWORD
            encoding:
              grant_type:
                style: form
              client_id:
                style: form
              client_secret:
                style: form
      responses:
        "200":
          description: Success
          content:
            text/plain:
              schema:
                $ref: '#/components/schemas/Authorization'
            application/json:
              schema:
                $ref: '#/components/schemas/Authorization'
            text/json:
              schema:
                $ref: '#/components/schemas/Authorization'
        "400":
          description: Bad Request
          content:
            text/plain:
              schema:
                type: string
            application/json:
              schema:
                type: string
            text/json:
              schema:
                type: string
        "401":
          description: Unauthorized
          content:
            text/plain:
              schema:
                type: string
            application/json:
              schema:
                type: string
            text/json:
              schema:
                type: string
        "429":
          description: Too Many Requests
          content:
            text/plain:
              schema:
                type: string
            application/json:
              schema:
                type: string
            text/json:
              schema:
                type: string
        "500":
          description: Internal Server Error
          content:
            text/plain:
              schema:
                type: string
            application/json:
              schema:
                type: string
            text/json:
              schema:
                type: string
